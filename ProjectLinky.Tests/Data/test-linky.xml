<?xml version="1.0"?>
<linky xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <projects path="BattlePillars.Droid.csproj">
    <rules inputPattern="Assets\Images\*.png" outputPattern="Assets\Images\" buildAction="AndroidAsset" />
  </projects>
  <projects path="BattlePillars.iOS.csproj">
    <rules inputPattern="Assets\Images\*.png" outputPattern="Content\Images\" buildAction="Content" />
  </projects>
</linky>